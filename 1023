#include <stdio.h>

int main(int argc, char *argv[]){
  int i;
  int input[10];
  for(i=0; i<10; i++){
    scanf("%d", &input[i]);
  }
  for(i=1; i<10; i++){
    if(input[i]!=0){
      printf("%d", i);
      input[i]--;
      break;
    }
  }
  int j;
  for(i=0; i<10; i++){
    for(j=0; j<input[i]; j++){
      printf("%d", i);
    }
  }
  printf("\n");
  return 0;
}
